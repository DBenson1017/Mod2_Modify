<h1>Song Index test </h1>



<%= form_tag(songs_path , :method => 'get') do%>
     <%= label_tag(:q, "Search for songs:") %>
     <%= text_field_tag(:search, params[:search]) %>
     <%= submit_tag("search")%>
 <% end %>
 

<%search = @tracks.last(5)%>
<%search.each do |s|%>
<p><%=link_to s.name, song_path(s.id)%></p>
<%end%>





